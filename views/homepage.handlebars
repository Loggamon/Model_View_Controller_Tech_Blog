{{#each posts as |post| }}
<div class="post-container">
  <div>
    <div class="post-title">
      <h2>
      <a href="/post/{{post.id}}">{{post.title}}</a>
      </h2>
      <p>{{user.name}} on {{format_date post.date_created}}</p>
    </div>
    <h3 class="description">{{description}}</h3>  
  </div>
</div>
{{/each}}